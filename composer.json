{
    "name": "soconnect/ruckus-client",
    "description": "Ruckus WLAN controller API client",
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": ">=7.2",
        "guzzlehttp/guzzle": "^6.3"
    },
    "require-dev": {
        "jakub-onderka/php-parallel-lint": "^1.0",
        "php-vcr/php-vcr": "^1.4",
        "phpstan/phpstan": "^0.10.6",
        "phpstan/phpstan-phpunit": "^0.10.0",
        "phpunit/phpunit": "^7",
        "squizlabs/php_codesniffer": "^3.3"
    },
    "autoload": {
        "psr-4": {
            "SoConnect\\RuckusClient\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Test\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "bin-dir": "bin"
    }
}
